<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service 
            id="cmf_tree_browser.phpcr_odm_tree" 
            class="Symfony\Cmf\Bundle\TreeBrowserBundle\Tree\PhpcrOdmTree"
            >
            <argument type="service" id="doctrine_phpcr.odm.default_document_manager" />
            <argument type="service" id="translator"/>

            <tag name="cmf.tree" alias="phpcr_odm_tree" />
        </service>

    </services>

</container>
